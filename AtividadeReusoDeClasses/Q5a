package AtividadeReusoDeClasses;

public class ObjetoGeometrico{
    private double L1;
    private double L2;
    private double L3;
    public ObjetoGeometrico(double L1, double L2, double L3){
        this.L1=L1;
        this.L2=L2;
        this.L3=L3;
        System.out.println(CalcularArea(L1,L2,L3));
        System.out.println(CalcularPerimetro(L1,L2,L3));
    }
    double CalcularArea(double L1, double L2, double L3){
        return L1*L2;
    }
    double CalcularPerimetro(double L1, double L2, double L3){
        return 2*(L1+L2);
    }
    public static void main(String[] args){
        retangulo R = new retangulo(1,2);
        Circulo C = new Circulo(10);
        triangulo T = new triangulo(3,5,6);
    }
}
